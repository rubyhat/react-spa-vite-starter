    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.1.5",
    "sass": "^1.85.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
