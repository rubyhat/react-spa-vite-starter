{
  "name": "react-spa-vite-starter",
  "private": true,
  "version": "0.0.9",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "prod": "vite --mode production",
    "build": "tsc && vite build --mode production",
    "build-stage": "tsc && vite build --mode staging",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 100",
    "test": "jest",
    "preview": "vite preview",
    "prepare": "husky",
    "precommit": "pnpm run format && pnpm run lint && pnpm run test && pnpm run gen-docs",
    "release": "release-it",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "gen-docs": "typedoc",
    "upgrade": "ncu -u && pnpm install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.1",
    "@tanstack/react-query": "^5.80.6",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.2",
    "sass": "^1.89.1",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.28.0",
    "@hookform/resolvers": "^5.1.1",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react-swc": "^3.10.1",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.13.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "react-icons": "^5.5.0",
    "release-it": "^19.0.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-jest": "^29.3.4",
    "typedoc": "^0.28.5",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1",
